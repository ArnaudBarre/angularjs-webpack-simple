{
  "name": "angularjs-webpack-simple",
  "description": "A simple boilerplate for angularjs using webapck and components",
  "version": "0.0.1",
  "author": "Arnaud Barr√© <arnaudbarremn@gamail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.10",
    "angular": "^1.6.6",
    "bootstrap": "^4.0.0-beta.2"
  },
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "angularjs-annotate"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-angularjs-annotate": "^0.8.2",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
